{
    "name": "aws-amplify",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "build": "tsc -b && vite build",
        "dev": "vite",
        "lint": "eslint .",
        "prepare": "husky install",
        "prettier:check": "prettier . --check",
        "preview": "vite preview",
        "test": "vitest"
    },
    "dependencies": {
        "aws-amplify": "^6.5.0",
        "react": "^18.3.1",
        "react-dom": "^18.3.1"
    },
    "devDependencies": {
        "@aws-amplify/backend": "^1.0.5",
        "@aws-amplify/backend-cli": "^1.2.3",
        "@commitlint/cli": "^19.4.1",
        "@commitlint/config-conventional": "^19.4.1",
        "@eslint/js": "^9.8.0",
        "@testing-library/dom": "^10.4.0",
        "@testing-library/jest-dom": "^6.5.0",
        "@testing-library/react": "^16.0.1",
        "@types/react": "^18.3.3",
        "@types/react-dom": "^18.3.0",
        "@vitejs/plugin-react": "^4.3.1",
        "aws-cdk": "^2.152.0",
        "aws-cdk-lib": "^2.152.0",
        "constructs": "^10.3.0",
        "esbuild": "^0.23.0",
        "eslint": "^9.8.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-react-hooks": "^5.1.0-rc.0",
        "eslint-plugin-react-refresh": "^0.4.9",
        "globals": "^15.9.0",
        "husky": "^9.1.4",
        "jsdom": "^25.0.0",
        "lint-staged": "^15.2.9",
        "prettier": "^3.3.3",
        "tsx": "^4.17.0",
        "typescript": "^5.5.4",
        "typescript-eslint": "^8.0.0",
        "vite": "^5.4.0",
        "vitest": "^2.0.5"
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx}": [
            "eslint --cache --fix",
            "prettier --write"
        ],
        "*.{html,css,json,md}": "prettier --write"
    }
}
